<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ls-sys.dicerosmovil" version="0.9.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DicerosMovil</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="gs@ls-sy.com" href="http://ls-sys.com">ls-sys</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
